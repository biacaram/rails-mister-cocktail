<div class='container mt-4'>
<h1>Cocktails</h1>

<% @cocktails.each do |cocktail| %>
<%= cl_image_tag cocktail.photo.key, height: 300 %>
<h4><%= link_to "#{cocktail.name}", cocktail_path(cocktail) %></h4>
<% end %>
<br>
<%= link_to 'Add cocktail', new_cocktail_path, class: 'btn btn-secondary' %>
</div>

